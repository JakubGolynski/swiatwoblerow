<?xml version="1.0" encoding="UTF-8"?>	
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xmlns:pro="http://www.liquibase.org/xml/ns/pro"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">
		
	<changeSet id="21" author="Goly">
		<createIndex indexName="IX_product_created_at" tableName="product" schemaName="shoproject">
			<column name="created_at"/>
		 </createIndex>
		 <createIndex indexName="IX_product_price" tableName="product" schemaName="shoproject">
			<column name="price"/>
		 </createIndex>
		 <createIndex indexName="IX_product_rating" tableName="product" schemaName="shoproject">
			<column name="rating"/>
		 </createIndex>
 	</changeSet>	
 	<changeSet id="22" author="Goly">
		<createIndex indexName="IX_category_name" tableName="category" schemaName="shoproject">
			<column name="name"/>
		 </createIndex>
 	</changeSet>	
 	<changeSet id="23" author="Goly">
		<createIndex indexName="IX_address_city" tableName="address" schemaName="shoproject">
			<column name="city"/>
		 </createIndex>
 	</changeSet>	
		
</databaseChangeLog>